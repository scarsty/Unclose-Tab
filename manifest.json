{
   "background": {
    "scripts": [ "background.js" ]
    },
   "browser_action": {
      "default_icon": "myicon.png",
      "default_title": "Unclose Tab!"
   },
   "description": "This extension will add a button which can reopen the recent closed tab by just one click.",
   "icons": {
      "128": "myicon.png",
      "48": "myicon48.png"
   },
   "options_ui": {
    "chrome_style": true,
    "page": "option.html"
    },
   "permissions": [ "tabs" ],
   "version": "0.2.0.0",
   "author": "SunTY",
   "manifest_version": 2,
   "name": "Unclose Tab"
}
